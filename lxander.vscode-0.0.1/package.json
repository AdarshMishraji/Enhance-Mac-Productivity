{
  "name": "lxander.vscode",
  "displayName": "Lxander VS Code",
  "description": "VS Code useful extensions",
  "version": "0.0.1",
  "publisher": "Lxander Corvus",
  "engines": {
    "vscode": "^1.74.3"
  },
  "categories": [
    "Themes",
    "Keymaps"
  ],
  "galleryBanner": {
    "color": "#000000",
    "theme": "dark"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "tags": [
    "js",
    "jsx",
    "osx",
    "git",
    "css",
    "dark",
    "blue",
    "icon",
    "scss",
    "html",
    "clean",
    "green",
    "macOS",
    "vivid",
    "theme",
    "yellow",
    "windows",
    "orginal",
    "balanced",
    "saturated",
    "beautiful",
    "dark theme",
    "color-theme",
    "web development"
  ],
  "contributes": {
    "commands": [
      {
        "command": "editor.action.goToReferences",
        "title": "‚Üí∆í Ref"
      },
      {
        "command": "workbench.action.togglePanel",
        "title": "‚¨ì Term"
      },
      {
        "command": "editor.action.rename",
        "title": "‚úé Rename"
      },
      {
        "command": "workbench.action.closeOtherEditors",
        "title": "‚ùå Close"
      },
      {
        "command": "workbench.action.toggleZenMode",
        "title": "‚ñ° Zen"
      },
      {
        "command": "workbench.files.action.showActiveFileInExplorer",
        "title": "‚äô Target"
      },
      {
        "command": "editor.action.organizeImports",
        "title": "üëá Imports"
      },
      {
        "command": "editor.foldAll",
        "title": "‚è´ Fold"
      },
      {
        "command": "editor.unfoldAll",
        "title": "‚è¨ UnFold"
      },
      {
        "command": "lxander.vscode.touchBar.enableFocusMode",
        "title": "üßò‚Äç‚ôÄÔ∏è Focus"
      },
      {
        "command": "workbench.files.action.focusFilesExplorer",
        "title": "Files",
        "icon": "./assets/images/files-activity-bar.png"
      },
      {
        "command": "workbench.view.search",
        "title": "Search",
        "icon": "./assets/images/search-activity-bar.png"
      },
      {
        "command": "workbench.view.scm",
        "title": "Source",
        "icon": "./assets/images/scm-activity-bar.png"
      },
      {
        "command": "workbench.view.debug",
        "title": "Debug",
        "icon": "./assets/images/debug-activity-bar.png"
      },
      {
        "command": "workbench.view.extensions",
        "title": "Extensions",
        "icon": "./assets/images/extensions-activity-bar.png"
      },
      {
        "command": "lxander.vscode.touchBar.gitcommit",
        "icon": "./assets/images/commit.png",
        "title": "Commit"
      },
      {
        "command": "lxander.vscode.touchBar.gitpush",
        "icon": "./assets/images/push.png",
        "title": "Push"
      },
      {
        "command": "lxander.vscode.touchBar.gitpull",
        "icon": "./assets/images/pull.png",
        "title": "Pull"
      },
      {
        "command": "lxander.vscode.touchBar.stashAll",
        "icon": "./assets/images/stash.png",
        "title": "StashAll"
      },
      {
        "command": "lxander.vscode.touchBar.stashLatestApply",
        "icon": "./assets/images/apply-stash.png",
        "title": "StashApply"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "editor.action.goToReferences",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.goToReferences"
        },
        {
          "command": "workbench.action.togglePanel",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.terminal"
        },
        {
          "command": "lxander.vscode.touchBar.enableFocusMode",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.focusMode"
        },
        {
          "command": "editor.action.rename",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.rename"
        },
        {
          "command": "workbench.action.closeOtherEditors",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.closeOtherEditors"
        },
        {
          "command": "workbench.action.toggleZenMode",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.toggleZenMode"
        },
        {
          "command": "workbench.files.action.showActiveFileInExplorer",
          "group": "group_2",
          "when": "config.lxander.vscode.touchBar.showActiveFileInExplorer"
        },
        {
          "command": "editor.action.organizeImports",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.organizeImports"
        },
        {
          "command": "editor.foldAll",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.foldAll"
        },
        {
          "command": "editor.unfoldAll",
          "group": "group_1",
          "when": "config.lxander.vscode.touchBar.unfoldAll"
        },
        {
          "command": "lxander.vscode.touchBar.gitcommit",
          "when": "config.lxander.vscode.touchBar.gitcommit",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.gitpush",
          "when": "config.lxander.vscode.touchBar.gitpush",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.gitpull",
          "when": "config.lxander.vscode.touchBar.gitpull",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.stashAll",
          "when": "config.lxander.vscode.touchBar.stashAll",
          "group": "activityBar"
        },
        {
          "command": "lxander.vscode.touchBar.stashLatestApply",
          "when": "config.lxander.vscode.touchBar.stashLatestApply",
          "group": "activityBar"
        },
        {
          "command": "workbench.files.action.focusFilesExplorer",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.showExplorer",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.search",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.search",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.scm",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.scm",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.debug",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.debug",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.extensions",
          "when": "config.lxander.vscode.touchBar.showActivityBar && config.lxander.vscode.touchBar.extensions",
          "group": "activityBar"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "TouchBar Pro",
      "properties": {
        "lxander.vscode.touchBar.goToReferences": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚Üí∆í Ref¬ª"
        },
        "lxander.vscode.touchBar.terminal": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚¨ì Term¬ª"
        },
        "lxander.vscode.touchBar.rename": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚úé Rename¬ª"
        },
        "lxander.vscode.touchBar.closeOtherEditors": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚§≠ Close¬ª"
        },
        "lxander.vscode.touchBar.focusMode": {
          "type": "boolean",
          "default": true,
          "description": "Enable Focus Mode"
        },
        "lxander.vscode.touchBar.toggleZenMode": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚ñ° Zen¬ª"
        },
        "lxander.vscode.touchBar.showActiveFileInExplorer": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚äô Target¬ª"
        },
        "lxander.vscode.touchBar.organizeImports": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚áä Imports¬ª"
        },
        "lxander.vscode.touchBar.foldAll": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚á† FoldAll¬ª"
        },
        "lxander.vscode.touchBar.unfoldAll": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚á¢ UnFold¬ª"
        },
        "lxander.vscode.touchBar.showActivityBar": {
          "type": "boolean",
          "default": false,
          "description": "Enables groupe buttons ¬´Activity bar¬ª"
        },
        "lxander.vscode.touchBar.showExplorer": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´File explorer¬ª of Activity bar"
        },
        "lxander.vscode.touchBar.search": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Search¬ª of Activity bar"
        },
        "lxander.vscode.touchBar.scm": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Source control¬ª of Activity bar"
        },
        "lxander.vscode.touchBar.debug": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Debug¬ª of Activity bar"
        },
        "lxander.vscode.touchBar.extensions": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Extensions¬ª of Activity bar"
        },
        "lxander.vscode.touchBar.gitcommit": {
          "type": "boolean",
          "default": true,
          "description": "Enable git commit"
        },
        "lxander.vscode.touchBar.gitpush": {
          "type": "boolean",
          "default": true,
          "description": "Enable git push"
        },
        "lxander.vscode.touchBar.gitpull": {
          "type": "boolean",
          "default": true,
          "description": "Enable git pull"
        },
        "lxander.vscode.touchBar.stashAll": {
          "type": "boolean",
          "default": true,
          "description": "Enable git stash"
        },
        "lxander.vscode.touchBar.stashLatestApply": {
          "type": "boolean",
          "default": true,
          "description": "Enable git stashLatestApply"
        }
      }
    },
    "themes": [
      {
        "label": "Lxander Dark Theme V1",
        "uiTheme": "vs-dark",
        "path": "./themes/v1.theme.json"
      },
      {
        "label": "Lxander Dark Theme V2",
        "uiTheme": "vs-dark",
        "path": "./themes/v2.theme.json"
      }
    ]
  },
  "extensionDependencies": [
    "vscode.git"
  ]
}