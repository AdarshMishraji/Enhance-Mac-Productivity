{
  "name": "touchbarvscode",
  "displayName": "Touch Bar ¬´TouchBar Pro¬ª",
  "description": "Support touch bar for MacBook pro",
  "publisher": "lxander",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.1"
  },
  "categories": [
    "Keymaps"
  ],
  "keywords": [
    "macbook pro",
    "extra buttons",
    "dark theme toggle",
    "touch bar",
    "amedomary"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "editor.action.goToReferences",
        "title": "‚Üí∆í Ref"
      },
      {
        "command": "workbench.action.togglePanel",
        "title": "‚¨ì Term"
      },
      {
        "command": "editor.action.rename",
        "title": "‚úé Rename"
      },
      {
        "command": "workbench.action.closeOtherEditors",
        "title": "‚ùå Close"
      },
      {
        "command": "workbench.action.toggleZenMode",
        "title": "‚ñ° Zen"
      },
      {
        "command": "workbench.files.action.showActiveFileInExplorer",
        "title": "‚äô Target"
      },
      {
        "command": "editor.action.organizeImports",
        "title": "üëá Imports"
      },
      {
        "command": "editor.foldAll",
        "title": "‚è´ Fold"
      },
      {
        "command": "editor.unfoldAll",
        "title": "‚è¨ UnFold"
      },
      {
        "command": "workbench.action.toggleSidebarVisibility",
        "title": "Side Bar"
      },
      {
        "command": "workbench.action.terminal.toggleTerminal",
        "title": "Terminal"
      },
      {
        "command": "lxander.touchbar.enableFocusMode",
        "title": "üßò‚Äç‚ôÄÔ∏è Focus"
      },
      {
        "command": "editor.action.copyLinesDownAction",
        "title": "‚ò∞‚Üì Copy"
      },
      {
        "command": "workbench.files.action.focusFilesExplorer",
        "title": "1Files",
        "icon": "./images/files-activity-bar.png"
      },
      {
        "command": "workbench.view.search",
        "title": "2Search",
        "icon": "./images/search-activity-bar.png"
      },
      {
        "command": "workbench.view.scm",
        "title": "3Source",
        "icon": "./images/scm-activity-bar.png"
      },
      {
        "command": "workbench.view.debug",
        "title": "4Debug",
        "icon": "./images/debug-activity-bar.png"
      },
      {
        "command": "workbench.view.extensions",
        "title": "5Extensions",
        "icon": "./images/extensions-activity-bar.png"
      },
      {
        "command": "lxander.touchbar.night",
        "title": "Night"
      },
      {
        "command": "lxander.touchbar.day",
        "title": "Day"
      },
      {
        "command": "lxander.touchbar.syncBranch",
        "title": "Git:Sync"
      },
      {
        "command": "lxander.touchbar.gitcommit",
        "title": "Git:Commit"
      },
      {
        "command": "lxander.touchbar.gitpush",
        "title": "Git:Push"
      },
      {
        "command": "lxander.touchbar.gitpull",
        "title": "Git:Pull"
      },
      {
        "command": "lxander.touchbar.checkoutTo",
        "title": "Git:Checkout"
      },
      {
        "command": "lxander.touchbar.stageAll",
        "title": "Git:Stage"
      }
    ],
    "menus": {
      "touchBar": [
        {
          "command": "editor.action.goToReferences",
          "group": "group_1",
          "when": "config.touchBar.goToReferences"
        },
        {
          "command": "workbench.action.togglePanel",
          "group": "group_2",
          "when": "config.touchBar.terminal"
        },
        {
          "command": "workbench.action.toggleSidebarVisibility",
          "group": "group_1",
          "when": "config.touchBar.focusCode"
        },
        {
          "command": "workbench.action.terminal.toggleTerminal",
          "group": "group_1",
          "when": "config.touchBar.focusCode"
        },
        {
          "command": "lxander.touchbar.enableFocusMode",
          "group": "group_1",
          "when": "config.touchBar.focusMode"
        },
        {
          "command": "editor.action.rename",
          "group": "group_1",
          "when": "config.touchBar.rename"
        },
        {
          "command": "workbench.action.closeOtherEditors",
          "group": "group_2",
          "when": "config.touchBar.closeOtherEditors"
        },
        {
          "command": "workbench.action.toggleZenMode",
          "group": "group_2",
          "when": "config.touchBar.toggleZenMode"
        },
        {
          "command": "workbench.files.action.showActiveFileInExplorer",
          "group": "group_2",
          "when": "config.touchBar.showActiveFileInExplorer"
        },
        {
          "command": "editor.action.organizeImports",
          "group": "group_1",
          "when": "config.touchBar.organizeImports"
        },
        {
          "command": "editor.foldAll",
          "group": "group_1",
          "when": "config.touchBar.foldAll"
        },
        {
          "command": "editor.unfoldAll",
          "group": "group_1",
          "when": "config.touchBar.unfoldAll"
        },
        {
          "command": "editor.action.copyLinesDownAction",
          "group": "group_1",
          "when": "config.touchBar.copyLinesDownAction"
        },
        {
          "command": "lxander.touchbar.syncBranch",
          "when": "config.touchBar.syncBranch",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.gitcommit",
          "when": "config.touchBar.gitcommit",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.gitpush",
          "when": "config.touchBar.gitpush",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.gitpull",
          "when": "config.touchBar.gitpull",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.checkoutTo",
          "when": "config.touchBar.checkoutTo",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.stageAll",
          "when": "config.touchBar.stageAll",
          "group": "activityBar"
        },
        {
          "command": "workbench.files.action.focusFilesExplorer",
          "when": "config.touchBar.showActivityBar && config.touchBar.showExplorer",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.search",
          "when": "config.touchBar.showActivityBar && config.touchBar.search",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.scm",
          "when": "config.touchBar.showActivityBar && config.touchBar.scm",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.debug",
          "when": "config.touchBar.showActivityBar && config.touchBar.debug",
          "group": "activityBar"
        },
        {
          "command": "workbench.view.extensions",
          "when": "config.touchBar.showActivityBar && config.touchBar.extensions",
          "group": "activityBar"
        },
        {
          "command": "lxander.touchbar.night",
          "when": "config.touchBar.themeToggle",
          "group": "themeToggle"
        },
        {
          "command": "lxander.touchbar.day",
          "when": "config.touchBar.themeToggle",
          "group": "themeToggle"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "TouchBar Pro",
      "properties": {
        "touchBar.goToReferences": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚Üí∆í Ref¬ª"
        },
        "touchBar.terminal": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚¨ì Term¬ª"
        },
        "touchBar.rename": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚úé Rename¬ª"
        },
        "touchBar.closeOtherEditors": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚§≠ Close¬ª"
        },
        "touchBar.focusCode": {
          "type": "boolean",
          "default": true,
          "description": "Enable Focus Code"
        },
        "touchBar.focusMode": {
          "type": "boolean",
          "default": true,
          "description": "Enable Focus Mode"
        },
        "touchBar.toggleZenMode": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚ñ° Zen¬ª"
        },
        "touchBar.showActiveFileInExplorer": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚äô Target¬ª"
        },
        "touchBar.organizeImports": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚áä Imports¬ª"
        },
        "touchBar.foldAll": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚á† FoldAll¬ª"
        },
        "touchBar.unfoldAll": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´‚á¢ UnFold¬ª"
        },
        "touchBar.copyLinesDownAction": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´‚ò∞‚Üì Copy¬ª"
        },
        "touchBar.showActivityBar": {
          "type": "boolean",
          "default": false,
          "description": "Enables groupe buttons ¬´Activity bar¬ª"
        },
        "touchBar.showExplorer": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´File explorer¬ª of Activity bar"
        },
        "touchBar.search": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Search¬ª of Activity bar"
        },
        "touchBar.scm": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Source control¬ª of Activity bar"
        },
        "touchBar.debug": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Debug¬ª of Activity bar"
        },
        "touchBar.extensions": {
          "type": "boolean",
          "default": true,
          "description": "Enables button ¬´Extensions¬ª of Activity bar"
        },
        "touchBar.themeToggle": {
          "type": "boolean",
          "default": false,
          "description": "Enables button ¬´Debug¬ª of Activity bar"
        },
        "touchBar.syncBranch": {
          "type": "boolean",
          "default": true,
          "description": "Enable git sync"
        },
        "touchBar.gitcommit": {
          "type": "boolean",
          "default": true,
          "description": "Enable git commit"
        },
        "touchBar.gitpush": {
          "type": "boolean",
          "default": true,
          "description": "Enable git push"
        },
        "touchBar.gitpull": {
          "type": "boolean",
          "default": true,
          "description": "Enable git pull"
        },
        "touchBar.checkoutTo": {
          "type": "boolean",
          "default": true,
          "description": "Enable git checkoutTo"
        },
        "touchBar.stageAll": {
          "type": "boolean",
          "default": true,
          "description": "Enable git stage"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.41.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}